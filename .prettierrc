{
  "plugins": ["prettier-plugin-go-template"],
  "overrides": [
    {
      "files": ["*.html"],
      "options": {
        "parser": "go-template"
      }
    }
  ],
  "singleQuote": true,
  "trailingComma": "none",
  "bracketSpacing": false,
  "semi": false,
  "singleAttributePerLine": true,
  "printWidth": 100
}
